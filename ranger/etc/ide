pathmunge () {
        if ! echo "$PATH" | grep -Eq "(^|:)$1($|:)" ; then
           if [ "$2" = "after" ] ; then
              PATH="$PATH:$1"
           else
              PATH="$1:$PATH"
           fi
        fi
}

pathmunge $PWD/bin

alias pull="git pull"
alias push="git commit -am pushing; git push"

PS1="<> $PS1"
